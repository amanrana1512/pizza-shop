Selector} from \"react-redux\";\nimport { addToCart } from \"../action/cartAction\";\n\nconst Pizza = ({ pizza }) => {\n  const [varient, setVarient] = useState(\"small\");\n  const [quantity, setQuantity] = useState(1);\n  const [show, setShow] = useState(false);\n\n  const dispatch=useDispatch()\n  \n  const addToCartHandler =()=> {\n    dispatch(addToCart(pizza,quantity,varient))\n  }\n\n  const handleClose = () => setShow(false);\n  const handleShow = () => setShow(true);\n\n  return (\n    <>\n      <Card style={{ width: \"18rem\", marginTop: \"30px\" }}>\n        <Card.Img variant=\"top\" src={pizza.image} \n            style={{height: \"200px\", cursor:\"pointer\"}}\n            onClick={handleShow}\n        />\n        <Card.Body>\n          <Card.Title>{pizza.name}</Card.Title>\n          <hr />\n          <Card.Text>\n            <Row>\n              <Col md=\"6\">\n                <h6>Varients</h6>\n                <select\n                  value={varient}\n                  onChange={(e) => setVarient(e.target.value)}\n                >\n                  {pizza.varients.map((varient) => (\n                    <option>{varient}</option>\n                  ))}\n                </select>\n              </Col>\n              <Col md=\"6\">\n                <h6>Quantity</h6>\n                <select\n                  value={quantity}\n                  onChange={(e) => setQuantity(e.target.value)}\n                >\n                  {[...Array(10).keys()].map((v, i) => (\n                    <option>{i + 1}</option>\n                  ))}\n                </select>\n              </Col>\n            </Row>\n          </Card.Text>\n          <Row>\n            <Col md=\"6\">Price: {pizza.prices[0][varient] * quantity} \\-</Col>\n            <Col md=\"6\">\n              <Button onClick={addToCartHandler} className=\"bg-warning text-white\">Add to Cart</Button>\n            </Col>\n          </Row>\n        </Card.Body>\n      </Card>\n      <Modal show={show} onHide={handleClose}>\n        <Modal.Header closeButton>\n          <Modal.Title>{pizza.name}</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n            <div>\n            <Card.Img variant=\"top\" src={pizza.image} \n            style={{height: \"200px\", cursor:\"pointer\"}}\n        />\n            </div>\n            <div>\n            {pizza.description}\n            </div>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button variant=\"secondary\" onClick={handleClose}>\n            Close\n          </Button>\n          <Button variant=\"primary\" onClick={handleClose}>\n            Save Changes\n          </Button>\n        </Modal.Footer>\n      </Modal>\n    </>\n  );\n};\n\nexport default Pizza;\n","/Users/amanrana/Desktop/Project1/pizza-shop/client/src/store.js",[],"/Users/amanrana/Desktop/Project1/pizza-shop/client/src/reducers/pizzaReducer.js",[],"/Users/amanrana/Desktop/Project1/pizza-shop/client/src/action/pizzaAction.js",[],"/Users/amanrana/Desktop/Project1/pizza-shop/client/src/reducers/cartReducer.js",[],"/Users/amanrana/Desktop/Project1/pizza-shop/client/src/action/cartAction.js",[],"/Users/amanrana/Desktop/Project1/pizza-shop/client/src/Screens/cartScreen.jsx",["113"],"/Users/amanrana/Desktop/Project1/pizza-shop/client/src/Screens/CartScreen.jsx",[],"/Users/amanrana/Desktop/Project1/pizza-shop/client/src/Screens/Register.jsx",["114","115"],"import React, { useState } from \"react\";\nimport { Container, Form, Button, Col } from \"react-bootstrap\";\nimport { useSelector,useDispatch } from \"react-redux\";\nimport {registerUser} from '../action/userAction';\nimport Loader from '../components/Loader';\nimport Success from '../components/Success';\nimport Error from '../components/Error';\n\nconst Register = () => {\n  const [name, setName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [confirmPassword, setConfirmPassword] = useState(\"\");\